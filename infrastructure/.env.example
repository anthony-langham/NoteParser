# SST Infrastructure Environment Variables
# Copy this to .env and fill in your values

# API Configuration
API_KEY=generate-a-secure-api-key-here
CORS_ORIGINS=https://heidimcp.uk,http://localhost:3000

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key

# Application Configuration
LOG_LEVEL=INFO

# Domain Configuration (for production)
DOMAIN_NAME=heidimcp.uk
HOSTED_ZONE=heidimcp.uk

# Development Configuration
DEV_CORS_ORIGINS=http://localhost:3000,http://localhost:5173